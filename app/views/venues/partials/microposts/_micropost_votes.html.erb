<% if fan_signed_in? && current_fan.venue_micropost_voted?(@micropost) %>
  <%= button_to "−", venue_micropost_vote_path(@venue, @micropost, current_fan.venue_micropost_vote_id(@micropost)), method: :delete, remote: true, class: "linwood" %>
<% elsif artist_signed_in? && current_artist.venue_micropost_voted?(@micropost) %>
  <%= button_to "−", venue_micropost_vote_path(@venue, @micropost, current_artist.venue_micropost_vote_id(@micropost)), method: :delete, remote: true, class: "linwood" %>
<% elsif record_label_signed_in? && current_record_label.venue_micropost_voted?(@micropost) %>
  <%= button_to "−", venue_micropost_vote_path(@venue, @micropost, current_record_label.venue_micropost_vote_id(@micropost)), method: :delete, remote: true, class: "linwood" %>
<% elsif venue_signed_in? && current_venue.venue_micropost_voted?(@micropost) %>
  <%= button_to "−", venue_micropost_vote_path(@venue, @micropost, current_venue.venue_micropost_vote_id(@micropost)), method: :delete, remote: true, class: "linwood" %>
<% elsif producer_signed_in? && current_producer.venue_micropost_voted?(@micropost) %>
  <%= button_to "−", venue_micropost_vote_path(@venue, @micropost, current_producer.venue_micropost_vote_id(@micropost)), method: :delete, remote: true, class: "linwood" %>
<% else %>
  <%= form_for(VenueMicropostVote.new, url: venue_micropost_votes_path(@venue, @micropost), remote: true) do |f| %>
    <%= f.submit "+", class: "linwood" %>
  <% end %>
<% end %>
