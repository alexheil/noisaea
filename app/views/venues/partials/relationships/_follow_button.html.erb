<% if fan_signed_in? && current_fan.following_venue?(@venue) %>
  <%= button_to "unfollow", venue_relationship_path(@venue, current_fan.venue_relationship_id(@venue)), method: :delete, remote: true, class: "exchange" %>
<% else %>
  <%= form_for(VenueRelationship.new,  url: venue_relationships_path(@venue), remote: true) do |f| %>
    <%= f.submit "follow", class: "exchange" %>
  <% end %>
<% end %>
