<% if @venue.venue_profile.cover_image.blank? %>
  <div class="clinton">
    <% if venue_signed_in? && @venue.id == current_venue.id %>
      you do not have a cover image yet
    <% else %>
      <%= "#{@venue.venue_name} hasn't uploaded a cover photo yet" %>
    <% end %>
  </div>
<% else %>
  <div class="broadway">
    <img srcset="<%= @venue.venue_profile.cover_image %>" width="100%" height="auto" />
  </div>
<% end %>
