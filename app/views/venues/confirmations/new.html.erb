<%= provide(:title, "Resend Confirmation Instructions | Noisaea //") %>
<%= render 'partials/uni_header' %>
<div class="buffalo">
  <div class="delaware">
    <div class="elmwood">
      <div class="harlem">
        <h1>resend confirmation instructions</h1>
      </div>

      <%= form_for(resource, as: resource_name, url: confirmation_path(resource_name), html: { method: :post }) do |f| %>
        <%= devise_error_messages! %>
        <div class="highgate">
          <div class="amherst temp">
            email
          </div>
          <%= f.email_field :email, autofocus: true, class: "huron" %>
        </div>
      <% end %>
      <div class="virginia">
        <%= render "artists/shared/links" %>
      </div>
    </div>
  </div>
</div>
<%= render 'partials/uni_footer' %>
