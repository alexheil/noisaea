<%= provide(:title, sanitize(@producer.producer_name) + " | Followers") %>
<div class="buffalo">

  <%= render 'producers/partials/producer_cover_image' %>

  <div class="delaware">

    <%= render 'producers/partials/producer_name' %>

    <% if @producer.fans.any? %>
    <div class="harlem">
        <% @producer.fans.take(100).each do |fan| %>
          <div class="utica">

            <%= render 'fans/partials/fan_profile_image', :fan => fan %>

            <div class="busti">
              <%= link_to fan.fan_name, fan_path(fan) %>
            </div>
          </div>
        <% end %>
    </div>
    <% end %>

  </div>
</div>
<%= render 'partials/uni_footer' %>
