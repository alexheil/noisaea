<% if fan_signed_in? && current_fan.producer_micropost_comment_voted?(comment) %>
  <%= button_to "−", producer_micropost_comment_vote_path(@producer, micropost, comment, current_fan.producer_micropost_comment_vote_id(comment)), method: :delete, remote: true, data: { disable_with: '...' }, class: "linwood" %>
<% elsif artist_signed_in? && current_artist.producer_micropost_comment_voted?(comment) %>
  <%= button_to "−", producer_micropost_comment_vote_path(@producer, micropost, comment, current_artist.producer_micropost_comment_vote_id(comment)), method: :delete, remote: true, data: { disable_with: '...' }, class: "linwood" %>
<% elsif record_label_signed_in? && current_record_label.producer_micropost_comment_voted?(comment) %>
  <%= button_to "−", producer_micropost_comment_vote_path(@producer, micropost, comment, current_record_label.producer_micropost_comment_vote_id(comment)), method: :delete, remote: true, data: { disable_with: '...' }, class: "linwood" %>
<% elsif venue_signed_in? && current_venue.producer_micropost_comment_voted?(comment) %>
  <%= button_to "−", producer_micropost_comment_vote_path(@producer, micropost, comment, current_venue.producer_micropost_comment_vote_id(comment)), method: :delete, remote: true, data: { disable_with: '...' }, class: "linwood" %>
<% elsif producer_signed_in? && current_producer.producer_micropost_comment_voted?(comment) %>
  <%= button_to "−", producer_micropost_comment_vote_path(@producer, micropost, comment, current_producer.producer_micropost_comment_vote_id(comment)), method: :delete, remote: true, data: { disable_with: '...' }, class: "linwood" %>
<% else %>
  <%= form_for(ProducerMicropostCommentVote.new, url: producer_micropost_comment_votes_path(@producer, micropost, comment), remote: true) do |f| %>
    <%= f.submit "+", data: { disable_with: '...' }, class: "linwood" %>
  <% end %>
<% end %>
