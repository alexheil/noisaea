<% if @artist.artist_profile.cover_img_file_name.blank? %>
  <div class="clinton">
    <% if @artist == current_artist %>
      you do not have a cover image yet
    <% else %>
      <%= "#{@artist.artist_name} hasn't uploaded a cover photo yet" %>
    <% end %>
  </div>
<% else %>
<div class="washington">
  <div style="background: url('<%= @artist.artist_profile.cover_img.url(:small) %>') no-repeat fixed; background-size: 100%;" class="broadway">
    <div class="northland">
      <%= @artist.artist_name %> <span class="cover_fan_count"><%= render 'artists/partials/relationships/cover_fan_count' %></span>
    </div>
  </div>
</div>
<div class="niagara">
  <div class="broadway">
    <img src="<%= @artist.artist_profile.cover_img.url(:small) %>" width="100%" height="auto'" />
  </div>
</div>
<% end %>
