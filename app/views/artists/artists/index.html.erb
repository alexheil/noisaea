      <div id="container-popular" class="ferry">
        <% @artists.sort_by{ |artist| -artist.fans.count }.take(15).each do |artist| %>
          <div class="utica">
            <% if artist.artist_profile.cover_image == "" %>
              <div class="sycamore">
                <% if artist_signed_in? && artist.id == current_artist.id %>
                  you do not have a profile image yet
                <% else %>
                  <%= "#{artist.artist_name} hasn't uploaded a profile image yet" %>
                <% end %>
              </div>
            <% else %>
              <div class="genesee">
                <img src="<%= artist.artist_profile.cover_image %>" width="100%" height="auto" />
              </div>
            <% end %>
              <div class="hertel">
                <center><%= link_to artist.artist_name, artist_path(artist) %></center>
              </div>
              <div class="hertel">
                <center><%= artist.artist_profile.genre unless artist.artist_profile.genre == "" %></center>
              </div>
              <div class="hertel">
                <center><%= artist.artist_profile.location unless artist.artist_profile.location == "" %></center>
              </div>
              <div class="hertel">
              <center><%= link_to pluralize(artist.fans.count, "fan"), artist_followers_path(artist) %></center>
